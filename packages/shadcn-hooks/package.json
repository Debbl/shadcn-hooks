  },
  "peerDependencies": {
    "react": ">=18"
  },
  "dependencies": {
    "@tanstack/react-query": "^5.90.21",
    "dequal": "^2.0.3",
    "es-toolkit": "^1.44.0",
    "swr": "^2.4.0",
    "use-mcp": "^0.0.21",
    "use-stick-to-bottom": "^1.1.3"
  },
  "devDependencies": {
    "@types/node": "^25.2.3",
    "shadcn": "^3.8.4",
