{
  "name": "shadcn-hooks",
  "type": "module",
  "version": "0.0.6",
  "packageManager": "pnpm@10.29.3",
  "description": "shadcn hooks",
  "author": "Brendan Dash <me@aiwan.run> (https://aiwan.run)",
  "license": "MIT",
  "homepage": "https://github.com/Debbl/shadcn-hooks#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Debbl/shadcn-hooks.git",
    "directory": "packages/shadcn-hooks"
  },
  "bugs": "https://github.com/Debbl/shadcn-hooks/issues",
  "keywords": [
    "react",
    "hooks",
    "shadcn",
    "react-use",
    "ahooks",
    "react-hooks-ts"
