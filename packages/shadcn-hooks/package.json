{
  "name": "shadcn-hooks",
  "type": "module",
  "version": "0.0.3",
  "packageManager": "pnpm@10.25.0",
  "description": "shadcn hooks",
  "author": "Brendan Dash <me@aiwan.run> (https://aiwan.run)",
  "license": "MIT",
  "homepage": "https://github.com/Debbl/shadcn-hooks#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Debbl/shadcn-hooks.git",
    "directory": "packages/shadcn-hooks"
  },
  "bugs": "https://github.com/Debbl/shadcn-hooks/issues",
  "keywords": [
    "react",
    "hooks",
    "shadcn",
    "react-use",
    "ahooks",
    "react-hooks-ts"
  ],
  "exports": {
    ".": "./dist/index.mjs",
    "./package.json": "./package.json"
  },
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.mts",
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "dev": "node ./src/index.ts",
    "build": "tsdown",
    "test": "vitest",
    "sync": "pnpm dlx shadcn@latest add @hooks/all --overwrite",
    "typecheck": "tsc --noEmit",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix"
  },
  "dependencies": {
    "@tanstack/react-query": "^5.90.12",
    "es-toolkit": "^1.42.0",
    "swr": "^2.3.7",
    "use-mcp": "^0.0.21",
    "use-stick-to-bottom": "^1.1.1"
  },
  "devDependencies": {
    "@types/node": "^24.10.2",
    "shadcn": "^3.5.1",
    "tsdown": "^0.17.2",
    "vitest": "^4.0.15"
  }
}
