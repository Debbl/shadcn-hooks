    "postcss": "^8.5.6",
    "prettier-plugin-tailwindcss": "^0.7.0",
    "remark": "^15.0.1",
    "remark-gfm": "^4.0.1",
    "remark-mdx": "^3.1.1",
    "serwist": "^9.2.1",
    "shadcn": "2.4.0-canary.12",
