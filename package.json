{
  "name": "shadcn-hooks",
  "type": "module",
  "version": "0.0.0",
  "private": true,
  "packageManager": "pnpm@10.18.3",
  "description": "shadcn hooks",
  "author": "Brendan Dash <me@aiwna.run> (https://github.com/Debbl/)",
  "license": "MIT",
  "homepage": "https://github.com/Debbl/shadcn-hooks",
  "repository": {
    "type": "git",
